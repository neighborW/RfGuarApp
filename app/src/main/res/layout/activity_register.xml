<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/bg_login"
    tools:context=".activity.RegisterActivity"
    android:orientation="vertical"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:layout_marginBottom="19.5dp"

        android:background="@drawable/shape_register_bg"
        android:orientation="vertical"
        >
        <include
            layout="@layout/common_back"
            android:layout_marginTop="29dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:layout_marginLeft="20dp"
           >
        </include>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="26.5dp"
            android:layout_marginRight="26dp"
            android:layout_gravity="center"
            android:layout_marginTop="40dp"
            android:background="@drawable/shape_register_line"
            android:orientation="horizontal">
            <EditText
                android:id="@+id/frstName"
                android:layout_width="135dp"
                android:layout_height="22.5dp"
                android:layout_weight="1"
                android:background="@null"
                android:hint="Frst Name"
                android:textColor="#000000"
                android:textColorHint="#bcbcbc"
                android:textSize="14dp" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="20dp">
        </View>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30.5dp"
            android:layout_marginRight="30dp"
            android:layout_gravity="center"
            android:background="@drawable/shape_register_line"
            android:orientation="horizontal">
            <EditText
                android:id="@+id/lastName"
                android:layout_width="135dp"
                android:layout_height="22.5dp"
                android:layout_weight="1"
                android:background="@null"
                android:hint="Last Name"
                android:textColor="#000000"
                android:textColorHint="#bcbcbc"
                android:textSize="14dp" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="20dp">
        </View>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30.5dp"
            android:layout_marginRight="30dp"
            android:layout_gravity="center"
            android:background="@drawable/shape_register_line"
            android:orientation="horizontal">
            <EditText
                android:id="@+id/agentName"
                android:layout_width="135dp"
                android:layout_height="22.5dp"
                android:layout_weight="1"
                android:background="@null"
                android:hint="Agent Name"
                android:textColor="#000000"
                android:textColorHint="#bcbcbc"
                android:textSize="14dp" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="20dp">
        </View>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30.5dp"
            android:layout_marginRight="30dp"
            android:layout_gravity="center"
            android:background="@drawable/shape_register_line"
            android:orientation="horizontal">
            <EditText
                android:id="@+id/contactNumber"
                android:layout_width="135dp"
                android:layout_height="22.5dp"
                android:layout_weight="1"
                android:background="@null"
                android:hint="Contact Number"
                android:textColor="#000000"
                android:textColorHint="#bcbcbc"
                android:textSize="14dp" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="20dp">
        </View>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30.5dp"
            android:layout_marginRight="30dp"
            android:layout_gravity="center"
            android:background="@drawable/shape_register_line"
            android:orientation="horizontal">
            <EditText
                android:id="@+id/et_postCode"
                android:layout_width="135dp"
                android:layout_height="22.5dp"
                android:layout_weight="1"
                android:background="@null"
                android:hint="Post Code"
                android:textColor="#000000"
                android:textColorHint="#bcbcbc"
                android:textSize="14dp" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="20dp">
        </View>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30.5dp"
            android:layout_marginRight="30dp"
            android:layout_gravity="center"
            android:background="@drawable/shape_register_line"
            android:orientation="horizontal">
            <EditText
                android:id="@+id/et_area"
                android:layout_width="135dp"
                android:layout_height="22.5dp"
                android:layout_weight="1"
                android:background="@null"
                android:hint="Area"
                android:textColor="#000000"
                android:textColorHint="#bcbcbc"
                android:textSize="14dp" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="20dp">
        </View>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30.5dp"
            android:layout_marginRight="30dp"
            android:layout_gravity="center"
            android:background="@drawable/shape_register_line"
            android:orientation="horizontal">
            <EditText
                android:id="@+id/et_address"
                android:layout_width="135dp"
                android:layout_height="22.5dp"
                android:layout_weight="1"
                android:background="@null"
                android:hint="Address"
                android:textColor="#000000"
                android:textColorHint="#bcbcbc"
                android:textSize="14dp" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="20dp">
        </View>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:layout_marginLeft="30.5dp"
            android:layout_marginRight="30dp"
            android:layout_marginBottom="33.5dp"
            android:background="@drawable/shape_big_login_btn"
            android:orientation="horizontal">
            <Button
                android:id="@+id/sumit"
                android:layout_width="135dp"
                android:layout_height="40dp"

                android:layout_weight="1"
                android:background="@null"
                android:hint="SIGN UP"
                android:textColor="#000000"
                android:textColorHint="#bcbcbc"
                android:textSize="14dp" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
