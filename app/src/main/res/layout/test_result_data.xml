<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/test_result_layout"
    android:background="@color/Color_Background"
>
    <!--//测试结果-->
    <LinearLayout
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@drawable/shape_border"
        >
        <TextView
            android:layout_marginLeft="12dp"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text="Test Result"
            android:textColor="@color/Color_Word85"
            android:gravity="center_vertical"
            />
        <ImageView
            android:id="@+id/split_imageview1"
            android:layout_width="match_parent"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            android:layout_height="1dp"
            android:scaleType="fitXY"
            android:src="@mipmap/dash_line" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center_vertical"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            android:orientation="horizontal"
            >
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:layout_weight="1"
                android:text="Test Group"
                android:textColor="@color/Color_Word45"
                android:textSize="14sp"
                />
            <TextView
                android:id="@+id/tv_test_group"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:textAlignment="textEnd"
                android:text="1000kPa"
                android:textSize="14sp"
                android:textColor="@color/Color_Word85"
                tools:ignore="RtlCompat" />
        </LinearLayout>
        <ImageView
            android:id="@+id/split_imageview2"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            android:scaleType="fitXY"
            android:src="@mipmap/dash_line" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            android:orientation="horizontal"
            >

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:text="Pressure at Start"
                android:textAlignment="textStart"
                android:textColor="@color/Color_Word45"
                android:textSize="@dimen/main_text_size"
                tools:ignore="RtlCompat" />
            <TextView
                android:id="@+id/tv_pre_at_start"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:textAlignment="textEnd"
                android:text="1000kPa"
                android:textSize="@dimen/main_text_size"
                android:textColor="@color/Color_Word85"
                tools:ignore="RtlCompat" />
        </LinearLayout>
        <ImageView
            android:id="@+id/split_imageview3"
            android:layout_width="match_parent"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            android:layout_height="1dp"
            android:scaleType="fitXY"
            android:src="@mipmap/dash_line" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            >
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:layout_weight="1"
                android:text="Pressure at End"
                android:textColor="@color/Color_Word45"
                android:textAlignment="textStart"
                android:textSize="@dimen/main_text_size"
                tools:ignore="RtlCompat" />
            <TextView
                android:id="@+id/tv_pressure_at_end"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="textEnd"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:text="1000kPa"

                android:textSize="@dimen/main_text_size"
                android:textColor="@color/Color_Word85"
                tools:ignore="RtlCompat" />
        </LinearLayout>
        <ImageView
            android:id="@+id/split_imageview4"
            android:layout_width="match_parent"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            android:layout_height="1dp"
            android:scaleType="fitXY"
            android:src="@mipmap/dash_line" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal"
            android:layout_marginRight="12dp"
            android:layout_marginLeft="12dp"
            >
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:layout_weight="1"
                android:text="Test Result"
                android:textColor="@color/Color_Word45"
                android:textAlignment="textStart"
                android:textSize="@dimen/main_text_size"
                tools:ignore="RtlCompat" />
            <TextView
                android:id="@+id/tv_statu"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textAlignment="textEnd"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:text="1000kPa"
                android:textSize="@dimen/main_text_size"
                android:textColor="@color/Color_Word85"
                tools:ignore="RtlCompat" />
        </LinearLayout>
        <ImageView
            android:id="@+id/split_imageview5"
            android:layout_width="match_parent"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            android:layout_height="1dp"
            android:scaleType="fitXY"
            android:src="@mipmap/dash_line" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:orientation="vertical"
            android:gravity="center"
            >
            <Button
                android:id="@+id/result_reset_btn"
                android:layout_width="90dp"
                android:layout_height="32dp"
                android:background="@drawable/shape_reset_bg"
                android:text="Reset"
                android:layout_gravity="center"
                android:textColor="@color/Color_Main"
                android:textSize="@dimen/button_text_size"
                />
        </LinearLayout>


    </LinearLayout>


</LinearLayout>